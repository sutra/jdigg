<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite
    PUBLIC "-//tuckey.org//DTD UrlRewrite 2.6//EN"
    "http://tuckey.org/res/dtds/urlrewrite2.6.dtd">
<urlrewrite>
	<rule>
		<from>^/allcats.html</from>
		<to>/categories.html</to>
	</rule>
	<rule>
		<from>^/(.*)-c.html</from>
		<to>/top-votes.html?categoryName=$1</to>
	</rule>
	<rule>
		<from>^/users/(.*)$</from>
		<to>/user.html?id=$1</to>
	</rule>
</urlrewrite>